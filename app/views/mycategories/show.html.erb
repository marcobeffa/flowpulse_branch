<%= render @mycategory %>

<div class="flex flex-wrap items-center gap mbs-6" style="--row-gap: .5rem">
  <%= link_to "Edit this mycategory", edit_mycategory_path(@mycategory), class: "btn" %>
  <%= link_to "Back to mycategories", mycategories_path, class: "btn" %>
  <%= button_to "Destroy this mycategory", @mycategory, method: :delete, class: "btn" %>
</div>
